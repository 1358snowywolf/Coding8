#include <bits/stdc++.h>

using namespace std;

int main(){
	vector<pair<int, int>> input;
	int N;
	cin >> N;
	int a, b;
	vector<int> parents;

	for(int i = 0; i < N; i++){
		cin >> a >> b;

		input.push_back(make_pair(a, b));
		parents.push_back(i);
	}

	sort(input.begin(), input.end());

	return 0;
}
