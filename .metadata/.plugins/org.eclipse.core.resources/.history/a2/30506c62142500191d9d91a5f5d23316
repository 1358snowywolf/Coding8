#include <bits/stdc++.h>

using namespace std;
map<int, pair<int, int>> past;

int main(){
	vector<pair<int, int>> input;
	int N;
	cin >> N;
	int a, b;
	vector<int> parents;

	for(int i = 0; i < N; i++){
		cin >> a >> b;

		input.push_back(make_pair(a, b));
		parents.push_back(i);
		past.insert(make_pair(b, make_pair(i, i)));
	}

	sort(input.begin(), input.end());
	vector<int> DP (input.size(), 0);

	DP[0] = 1;

	for(int i = 1; i < N; i++){
		int bestIndex = i;
		past[input[i].second].second = i;

		for(int j = i - 1; j >= 0; j--){
			if(DP[bestIndex] < DP[j] && input[i].second < input[j].second){
				bestIndex = j;
			}
		}
	}

	return 0;
}
